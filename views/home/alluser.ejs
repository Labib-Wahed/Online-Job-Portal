<!DOCTYPE html>
<html>
<head>
	<title>Home Page</title>
</head>
<body>
	<div>
		<h1>User List</h1> 		
	</div>

	<a href="/home"> Back</a> |
	<a href="/logout"> logout</a><br>

	<table border="1">
		<tr>
			<td>eid</td>
			<td>employeeName</td>
			<td>companyName </td>
			<td>contractNo</td>
			<td>userName</td>
			<td>Password</td>
			<td>Action</td>
		</tr>
		<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].eid %></td>
			<td><%= userlist[i].employeeName %></td>
			<td><%= userlist[i].companyName %></td>	
			<td><%= userlist[i].contractNo %></td>
			<td><%= userlist[i].userName %></td>
			<td><%= userlist[i].password %></td>
			<td>
				<a href="/home/edit/<%= userlist[i].eid %>">EDIT</a> | 
				<a href="/home/delete/<%= userlist[i].eid %>">DELETE</a>  
			</td>
		</tr>
		<% } %>
	</table>
</body>
</html>